{
  "name": "threewzrd",
  "version": "1.0.2",
  "description": "AI-powered CLI for generating Three.js projects from natural language",
  "main": "dist/cli.js",
  "bin": {
    "threewzrd": "./dist/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
