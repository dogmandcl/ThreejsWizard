  "author": "DOGMAN DEVELOPMENT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dogmandcl/ThreejsWizard"
  },
  "bugs": {
    "url": "https://github.com/dogmandcl/ThreejsWizard/issues"
  },
  "homepage": "https://github.com/dogmandcl/ThreejsWizard#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
