    "start": "node dist/cli.js start",
    "dev": "tsc && node dist/cli.js start",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "threejs",
    "ai-agent",
    "claude",
    "3d",
    "webgl",
    "cli",
